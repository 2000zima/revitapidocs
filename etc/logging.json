  {
    "version": 1,
    "disable_existing_loggers": true,
    "handlers": 
    {
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "simple"
                },

        "file_latest": {
           "class": "logging.FileHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "filename": "../log_latest.log",
            "mode": "w"
                },

        "info_file_handler": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "INFO",
            "formatter": "simple",
            "filename": "logs/info.log",
            "maxBytes": 10485760,
            "backupCount": 20,
            "encoding": "utf8"
                            },

        "error_file_handler": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "ERROR",
            "formatter": "complete",
            "filename": "logs/errors.log",
            "maxBytes": 10485760,
            "backupCount": 20,
            "encoding": "utf8"
                            }
    },

    "formatters": {
        "simple": {
            "format": "%(asctime)s[%(levelname)s] \t %(message)s [%(filename)s](%(lineno)d)",
            "datefmt" : "%I:%M:%S"
            },
        "complete": {
            "format": "%(asctime)s [%(filename)s] - %(levelname)s - %(message)s: Line %(lineno)d [logger: %(name)s / modeule:%(module)s]",
            "datefmt" : "%Y/%m/%d %I:%M:%S %p"
                    }
    },
    "loggers": {
            "": {
                    "handlers": ["console", "file_latest","info_file_handler","error_file_handler"],
                    "level": "DEBUG"
                }
               }
    }